{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "typescript-config/consistent-casing": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "border: 1px solid color-mix(in oklab, var(--mat-sys-error) 40%, transparent)"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}